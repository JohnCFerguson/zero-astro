{
	"name": "zero-astro",
	"type": "module",
	"version": "0.0.1",
	"description": "Zero Sync integration for Astro",
	"author": "John Ferguson",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/JohnCFerguson/hello-zero.git"
	},
	"keywords": [
		"astro-integration",
		"astro",
		"zero-sync",
		"replication"
	],
	"scripts": {
		"build": "tsc && esbuild src/index.ts --bundle --format=esm --outfile=dist/index.js",
		"dev": "astro dev",
		"preview": "astro preview",
		"astro": "astro",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf dist"
	},
	"dependencies": {
		"@rocicorp/zero": "^0.11.2025011402",
		"astro": "^5.1.7"
	},
	"devDependencies": {
		"@eslint/js": "^9.18.0",
		"@types/eslint-config-prettier": "^6.11.3",
		"@typescript-eslint/eslint-plugin": "^8.20.0",
		"@typescript-eslint/parser": "^8.20.0",
		"esbuild": "^0.24.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-astro": "^1.3.1",
		"globals": "^15.14.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.7",
		"vitest": "^2.1.8"
	},
	"peerDependencies": {
		"@rocicorp/zero": "^0.10.0",
		"astro": "^5.1.7"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"main": "./dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	]
}
