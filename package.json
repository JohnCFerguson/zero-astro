{
	"name": "zero-astro",
	"type": "module",
	"version": "0.0.1",
	"description": "Zero integration for Astro",
	"author": "Your Name",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/username/zero-astro.git"
	},
	"keywords": [
		"astro",
		"integration",
		"zero"
	],
	"scripts": {
		"build": "rm -rf dist && tsc && node esbuild.config.mjs",
		"types": "tsc --emitDeclarationOnly --outDir dist",
		"dev": "astro dev",
		"preview": "astro preview",
		"astro": "astro",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf dist",
		"prepublishOnly": "pnpm build"
	},
	"dependencies": {
		"@rocicorp/zero": "^0.11.2025011402",
		"astro": "^5.1.7"
	},
	"devDependencies": {
		"@eslint/js": "^9.18.0",
		"@types/eslint-config-prettier": "^6.11.3",
		"@typescript-eslint/eslint-plugin": "^8.20.0",
		"@typescript-eslint/parser": "^8.20.0",
		"esbuild": "^0.24.2",
		"esbuild-plugin-d.ts": "^1.3.1",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-astro": "^1.3.1",
		"globals": "^15.14.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.7",
		"vitest": "^2.1.8"
	},
	"peerDependencies": {
			"astro": "^5.1.7",
			"@rocicorp/zero": "^0.11.2025011402"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"main": "./dist/index.js",
	"module": "dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"packageManager": "pnpm@10.0.0"
}
